<Window x:Class="StartProcessTime.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartProcessTime.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="473" Width="400">
    <Grid Margin="0,0,0,-6">

        <StackPanel HorizontalAlignment="Center" Margin="0,20">
            <Button Margin="0,15" FontSize="20" Width="350" Height="50" Click="ButtonBegin_Click" FontWeight="Bold" Focusable="False">НАЧАТЬ ЗАХВАТ ПРОЦЕССА</Button>
            <Button Margin="0,15" FontSize="20" Width="350" Height="50" Click="ButtonChart_Click" FontWeight="Bold" Focusable="False">ГРАФИКИ</Button>
            <Button Margin="0,15" FontSize="20" Width="350" Height="50" Click="ButtonAddProcess_Click" Focusable="False">ДОБАВИТЬ ПРОЦЕСС</Button>
            <Button Margin="0,15" FontSize="20" Width="350" Height="50" Click="ButtonExit_Click" Focusable="False">ВЫХОД</Button>
        </StackPanel>


    </Grid>
</Window>
